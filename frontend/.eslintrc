{
  "extends": [
    "airbnb",
    "equimper"
  ],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "vars": "local",
        "args": "none"
      }
    ],
    "react/jsx-filename-extension": 0,
    "jsx-a11y/anchor-is-valid":
      [ "off", {
        "components": [ "Link" ]
      }]
  }
}
